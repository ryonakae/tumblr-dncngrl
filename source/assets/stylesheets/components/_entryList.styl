.entryList
  opacity 0
  list-style-type none
  margin 0 auto
  padding 0
  transition: all 600ms easing('out-quint')
  transform translateY(100px)

  &.is--visible
    opacity 1
    transform translateY(0)

  &--photo
    padding-bottom 100px


.entryList__item
  &--photo
    margin 0 auto 100px

    &:last-child
      margin-bottom 0